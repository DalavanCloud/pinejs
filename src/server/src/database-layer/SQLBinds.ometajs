define(['ometa-core'], function() {
	ometa SQLBinds {
		skipToEnd :quote =
			(	(	seq(quote)
				|	'\\'
				)
				seq(quote)
			|	~seq(quote)
				anything
			)*,
		parse :nextBind =
			(
				<	('\''|'"'):quote
					skipToEnd(quote)
					seq(quote)
				>
			|	'?'
				-> nextBind()
			|	anything
			)*:sql
			-> sql.join('')
	}
	return SQLBinds;
});