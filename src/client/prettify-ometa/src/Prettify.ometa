define(['ometa/ometa-base'], function() {
	var Prettify;
	//Converts a tree to a nested list.

	ometa Prettify {
		Elem :indent =
			[
				(	string:s
					-> { "'" + s + "'" }
				|	Elem(indent + '\t')
				|	number
				|	true
				|	false
				)*:e
			]
			-> ('[' + e.join(',\n' + indent) + ']'),
		Process =
			Elem('\t')
	}

	return Prettify;
})